procedure main()
    intext := open("todo.md")
    outtext := open("tod.md","w")
    while line := read(intext) do {
        if isGood(copy(line)) ~= 0 then {
            write(outtext,line)
        }
    }
    write("end")
end

procedure isGood(s)
    if match(s[1:6],"[‚ùé]") then {
        return 1
    }
    return 0
end